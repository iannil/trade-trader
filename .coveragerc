[run]
source = trade_trader
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */site-packages/*
    */migrations/*
    */venv/*
    */virtualenv/*
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
